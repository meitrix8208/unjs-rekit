{
  "name": "unjs-rekit",
  "version": "0.0.4",
  "description": "a fork from unjs/unkit",
  "repository": "git+https://github.com/meitrix8208/re-kit.git",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    "./env": {
      "import": "./dist/env.mjs",
      "require": "./dist/env.cjs",
      "types": "./dist/env.d.ts"
    },
    "./fetch": {
      "import": "./dist/fetch.mjs",
      "require": "./dist/fetch.cjs",
      "types": "./dist/fetch.d.ts"
    },
    "./string": {
      "import": "./dist/string.mjs",
      "require": "./dist/string.cjs",
      "types": "./dist/string.d.ts"
    },
    "./url": {
      "import": "./dist/url.mjs",
      "require": "./dist/url.cjs",
      "types": "./dist/url.d.ts"
    },
    "./object": {
      "import": "./dist/object.mjs",
      "require": "./dist/object.cjs",
      "types": "./dist/object.d.ts"
    },
    "./crypto": {
      "import": "./dist/crypto.mjs",
      "require": "./dist/crypto.cjs",
      "types": "./dist/crypto.d.ts"
    },
    "./https": {
      "import": "./dist/https.mjs",
      "require": "./dist/https.cjs",
      "types": "./dist/https.d.ts"
    },
    "./cli": {
      "import": "./dist/cli.mjs",
      "require": "./dist/cli.cjs",
      "types": "./dist/cli.d.ts"
    },
    "./regex": {
      "import": "./dist/regex.mjs",
      "require": "./dist/regex.cjs",
      "types": "./dist/regex.d.ts"
    },
    "./path": {
      "import": "./dist/path.mjs",
      "require": "./dist/path.cjs",
      "types": "./dist/path.d.ts"
    },
    "./*": "./dist/*"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "prepack": "pnpm build --minify",
    "lint": "eslint --cache --ext .ts,.js,.mjs,.cjs . && prettier -c src",
    "lint:fix": "eslint --cache --ext .ts,.js,.mjs,.cjs . --fix && prettier -c src -w",
    "release": "pnpm lint && pnpm test && changelogen --release && git push --follow-tags && pnpm publish",
    "test": "tsc --noEmit --skipLibCheck"
  },
  "devDependencies": {
    "@types/node": "20.11.0",
    "changelogen": "0.5.5",
    "eslint-config-unjs": "0.2.1",
    "eslint": "8.56.0",
    "prettier": "3.2.2",
    "typescript": "5.3.3",
    "unbuild": "2.0.0"
  },
  "packageManager": "pnpm@8.14.1",
  "dependencies": {
    "c12": "1.6.1",
    "citty": "0.1.5",
    "consola": "3.2.3",
    "defu": "6.1.4",
    "destr": "2.0.2",
    "express-get-is-https": "0.0.2",
    "express-get-url": "0.0.3",
    "h3": "1.10.0",
    "magic-regexp": "0.7.0",
    "ofetch": "1.3.3",
    "ohash": "1.1.3",
    "pathe": "1.1.2",
    "scule": "1.2.0",
    "std-env": "3.7.0",
    "ufo": "1.3.2",
    "uncrypto": "0.1.3"
  }
}